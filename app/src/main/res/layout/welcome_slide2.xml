<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:gravity="center_horizontal"
    android:orientation="vertical">

    <ImageView
        android:layout_width="298dp"
        android:layout_height="243dp"
        android:layout_marginTop="90dp"
        android:src="@drawable/group_2448"
        tools:ignore="ContentDescription" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:text="@string/consult_over_video_call_chat_or_visit"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textFontWeight="600"
        android:textSize="34sp"
        android:textStyle="normal"
        tools:ignore="UnusedAttribute" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:paddingLeft="40dp"
        android:paddingRight="40dp"
        android:text="@string/unwell_or_looking_for_second_opinion_nget_consulation_with_the_doctor_as_per_nyour_prefrence"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="16sp" />

</LinearLayout>